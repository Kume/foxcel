{
  "name": "@foxcel/core",
  "version": "0.0.1",
  "description": "Core module for foxcel",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "author": "makoto.kume",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src"
  },
  "dependencies": {
    "js-yaml": "^3.14.1",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^3.12.7",
    "@types/lodash.pick": "^4.4.7",
    "eslint": "^6.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./src/.+(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
