{
  "name": "@foxcel/core",
  "version": "0.0.1",
  "description": "Core module for foxcel",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "author": "makoto.kume",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json -w",
    "typecheck": "tsc",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@swc/core": "^1.4.12",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.pick": "^4.4.9",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "sourceMaps": "inline"
        }
      ]
    },
    "testRegex": "./src/.+(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "volta": {
    "node": "20.12.1"
  }
}
