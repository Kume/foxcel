{
  "name": "@foxcel/core",
  "version": "0.0.1",
  "description": "Core module for foxcel",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./src/index.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "author": "makoto.kume",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json -w",
    "typecheck": "tsc",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.64",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.pick": "^4.4.7",
    "eslint": "^6.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "sourceMaps": "inline"
        }
      ]
    },
    "testRegex": "./src/.+(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "volta": {
    "node": "20.12.1"
  }
}
