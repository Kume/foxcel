{
  "name": "co-doc-editor-core",
  "version": "0.0.1",
  "description": "Core module for co-doc-editor",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "author": "makoto.kume",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src"
  },
  "dependencies": {
    "lodash.pick": "^4.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./src/.+(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
